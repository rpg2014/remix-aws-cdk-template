{
  "private": true,
  "name": "remix-app-template",
  "description": "A remix app implemented on Lambda@Edge using cdk",
  "license": "MIT",
  "version": "1.0",
  "scripts": {
    "build": "yarn build:css && vite build && vite build --ssr && echo 'Add or remove build steps to here as needed'",
    "build:css": "sass --no-source-map -s compressed app/ ",
    "build:rust-functions": "wasm-pack build rust-functions --target nodejs --out-name rust-functions",
    "release": "yarn lint && yarn build",
    "dev:css": "sass --watch app/",
    "dev": "concurrently --kill-others 'yarn dev:css' 'vite dev'",
    "deploy": "yarn build && cdk deploy --profile personal",
    "diff": "yarn build && cdk diff --profile personal",
    "synth": "cdk synth",
    "postinstall": "remix setup node",
    "start": "remix-serve build/server",
    "clean": "rm -rf build node_modules rust-functions/pkg",
    "test": "jest .",
    "lint": "yarn prettier . --write && yarn run eslint . --fix"
  },
  "dependencies": {
    "@remix-run/node": "^2.3.1",
    "@remix-run/react": "^2.3.1",
    "@remix-run/serve": "^2.3.1",
    "@remix-run/server-runtime": "^2.3.1",
    "@types/node": "^20.6.0",
    "@types/source-map-support": "^0.5.7",
    "esbuild": "^0.19.2",
    "isbot": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix": "^2.0.0",
    "roadhouse-wasm": "github:rpg2014/roadhouse-wasm",
    "rust-functions": "./rust-functions/pkg",
    "swr": "^2.2.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.3.1",
    "@remix-run/eslint-config": "^2.3.1",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^27.0.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "aws-cdk": "^2.96.2",
    "aws-cdk-lib": "^2.96.2",
    "concurrently": "^8.2.1",
    "constructs": "^10.2.70",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.6.4",
    "prettier": "3.0.3",
    "sass": "^1.67.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.6",
    "vite-tsconfig-paths": "^4.2.2"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "packageManager": "yarn@4.0.2"
}
