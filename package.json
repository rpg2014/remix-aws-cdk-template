{
  "private": true,
  "name": "remix-app-template",
  "description": "A remix app implemented on Lambda@Edge using cdk",
  "license": "MIT",
  "version": "1.0",
  "scripts": {
    "build": "remix build",
    "release": "yarn lint && yarn build",
    "dev": "remix dev",
    "deploy": "yarn build && cdk deploy --profile personal",
    "diff": "yarn build && cdk diff --profile personal",
    "postinstall": "remix setup node",
    "start": "remix-serve build/server",
    "clean": "rm -rf build",
    "lint": "yarn prettier . --write && yarn run eslint . --fix"
  },
  "dependencies": {
    "@remix-run/css-bundle": "^1.19.3",
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-run/serve": "^1.19.3",
    "@remix-run/server-runtime": "^1.19.3",
    "@types/node": "^20.6.0",
    "esbuild": "^0.19.2",
    "isbot": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix": "^1.19.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.3",
    "@remix-run/eslint-config": "^1.19.3",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^27.0.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "aws-cdk": "^2.95.1",
    "aws-cdk-lib": "^2.95.1",
    "constructs": "^10.2.70",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.6.4",
    "prettier": "3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "packageManager": "yarn@3.6.3"
}
